---

import NavBar from '../layouts/JBNavBar.astro';
// import NavBar from '../layouts/NavBar.astro';
import ExampleMainContent from '../components/jb.example.main.content.astro';
export interface Props {
	title: string;
}

const { title } = Astro.props;
---



<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description">
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>


	</head>
	<body>
		
        <NavBar image="https://www.locofy.ai/blog/locofylogo.svg"/>
        
        

        <div class="px-10 py-10 w-1/2 justify-between mx-auto my-auto">

        <!-- main content /-->
        <!-- <ExampleMainContent example="3forges example"/> -->
        <slot />
        <!-- END main content /-->

        </div>
            

    </div>

    </body>

    <!-- Js to execute when document is ready -->
    <script type="text/javascript">
    // $(function () {
      // var ch = $("#chart");
      var ch = document.getElementById("chart");
      var ch_data = [
        { year: "2008", value: 20 },
        { year: "2009", value: 10 },
        { year: "2010", value: 15 },
        { year: "2011", value: 8 },
        { year: "2012", value: 21 }
      ];
      if (ch.length && ch.length > 0) {
        new Morris.Bar({
          element: ch,
          data: ch_data,
          xkey: "year",
          ykeys: ["value"],
          labels: ["Value"]
        });
      }
      
      // var ot = $("#report");
      // var ch = $("#chart");
      var ot = document.getElementById("report");
      var ot_data = [
        { year: "2008", value: 20 },
        { year: "2009", value: 10 },
        { year: "2010", value: 15 },
        { year: "2011", value: 8 },
        { year: "2012", value: 21 }
      ];
      if (ot.length && ot.length > 0) {
        new Morris.Line({
          element: ot,
          data: ot_data,
          xkey: "year",
          ykeys: ["value"],
          labels: ["Value"]
        });
      }
      
    // });
    
    </script>

